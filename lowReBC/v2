FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      v2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 1e-6;

boundaryField
{
    "wall.*"
    {
        type            v2WallFunction;
        value           $internalField;
    }
    "empty.*"
    {
        type        empty;
    }
    "proc.*"
    {
      type processor;
    }
}


// ************************************************************************* //

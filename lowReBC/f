FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 1e-12;

boundaryField
  {
    "wall.*"
    {
      type            fWallFunction;
      value           $internalField;
    }
    "empty.*"
    {
      type            empty;
    }
    "proc.*"
    {
      type processor;
    }
  }


